from turtle import *
from objects import *
speed(10)
pensize(4)
color('light blue')
begin_fill()
goto(-247,0)
left (90)
forward(337)
right(90)
forward(495)
right(90)
forward(337)
right(90)
forward(250)
end_fill()
goto(-247,0)
left(90)
color('lime')
begin_fill()
forward(100)
left(90)
forward(495)
left(90)
forward(100)
left(90)
forward(250)
end_fill()

right(180)
penup()
goto(-200,0)

bighouse()

goto(-70,0)
house()
penup()
left(60)
goto(150,0)
pendown()
tree()

exitonclick()
